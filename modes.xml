  <mode name="sme-sma" install="yes">
    <pipeline>
      <program name="hfst-proc --weight-classes 1 -w -e">
        <file name="sme-sma.automorf.hfst"/>
      </program>
      <program name="cg-proc -w">
        <file name="sme-sma.mor.rlx.bin"/>
