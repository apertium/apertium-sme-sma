    <def-macro n="aehtjie-gon-tjidtjiehParam3" npar="3"> 
      <choose>
      <when c="M[T] nom gets pl">
      <test><equal><clip pos="3" side="tl" part="a_case"/><lit-tag v="nom"/></equal></test>
      <let><clip pos="3" side="tl" part="a_nbr"/><lit-tag v="pl"/></let>
      </when>
       </choose>
      <choose>
      <when c="M[T] ill to nom">
      <test><equal><clip pos="1" side="tl" part="a_case"/><lit-tag v="ill"/></equal></test>
      <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/></let>
      </when>
      <when c="M[T] loc to nom">
      <test><equal><clip pos="1" side="tl" part="a_case"/><lit-tag v="loc"/></equal></test>
      <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/></let>
         <call-macro n="convert-case-nbr-poss-1Param"><with-param pos="3"/></call-macro>      
      </when>
       </choose>
    </def-macro>

    <def-macro n="convert-nomact-1Param" npar="1"> 
      <choose>  
        <when c="M[L] noun to verb: sirdin to juhtedh">
          <test><equal>
            <clip pos="1" side="sl" part="lem"/><lit v="sirdin"/></equal></test>
          <let><clip pos="1" side="tl" part="lem"/><lit v="juhtedh"/></let>
        </when>
        <when c="M[L] noun to verb: ovddideapmi to evtiedidh">
          <test><equal>
            <clip pos="1" side="sl" part="lem"/><lit v="ovddideapmi"/></equal></test>
          <let><clip pos="1" side="tl" part="lem"/><lit v="evtiedidh"/></let>
        </when>
        <when c="M[L] noun to verb: árvvoštallan to vierhtiedalledh">
          <test><equal>
            <clip pos="1" side="sl" part="lem"/><lit v="árvvoštallan"/></equal></test>
          <let><clip pos="1" side="tl" part="lem"/><lit v="vierhtiedalledh"/></let>
        </when>
        <when c="M[L] noun to verb: gulahallan to gaskesadtedh">
          <test><equal>
            <clip pos="1" side="sl" part="lem"/><lit v="gulahallan"/></equal></test>
          <let><clip pos="1" side="tl" part="lem"/><lit v="gaskesadtedh"/></let>
        </when>
        <when c="M[L] noun to verb: huksen to tseegkedh">
          <test><equal>
            <clip pos="1" side="sl" part="lem"/><lit v="huksen"/></equal></test>
          <let><clip pos="1" side="tl" part="lem"/><lit v="tseegkedh"/></let>
        </when>
      </choose>
    </def-macro>
    <def-macro n="convert-deriv-1Param" npar="1"> <!-- der_aadv to der_laakan -->
      <choose>  
        <when c="M[T] caus to der_ht">
          <test>
            <in caseless="yes"><clip pos="1" side="sl" part="lem"/><list n="der_caus2der_d"/></in></test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_ht"/></let>
        </when>
        <when c="M[T] der_lasj.adj to sg.gen">
          <test><and>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_lasj.adj"/></equal>
               <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="n"/></equal>
          </and></test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit v=""/></let>
          <let><clip pos="1" side="tl" part="a_attr"/><lit-tag v="sg.gen"/></let>
        </when>
        <when c="M[T] der_aadv to der_laakan">
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_aadv"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_laakan"/></let>
        </when>
        <when c="M[T] der_vuota: remove a_attr">
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_vuota"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_attr"/><lit v=""/></let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="set_caseFirstword" npar="1"
               c="Set the typographic case to be carried by the chunk.
                  - if argument is _not_ a proper noun: caseFirstword is the case of the lemma, and the lemma gets lowercase.
                  - if argument _is_ a proper noun: caseFirstword is lowercase, and lemma stays unchanged.

                  We treat UPPERCASE as proper nouns.">
      <let><var n="caseFirstword"/><lit v="aa"/></let>
      <choose><when>
        <test><not><or>
          <begins-with><clip pos="1" side="tl" part="a_nom"/><lit-tag v="np"/></begins-with>
          <!-- <equal><clip pos="1" side="tl" part="lettercase"/><lit-tag v="fixedcase"/></equal> -->
          <equal><get-case-from pos="1"><lit v="aa"/></get-case-from><lit v="AA"/></equal>
        </or></not></test>
        <let><var n="caseFirstword"/><get-case-from pos="1"><lit v="aa"/></get-case-from></let>
        <modify-case><clip pos="1" side="tl" part="lemh"/><lit v="aa"/></modify-case>
      </when></choose>
    </def-macro>

    <def-macro n="general-macro-1Param" npar="1">
      <choose>
        <when c="M[V] set main-verb-lemma">
          <test>
            <and><or>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@-FMAINV"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@+FMAINV"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@←ADVL"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@←SUBJ"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@A←"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@N←"/></equal></or>
              <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="vblex"/></equal>
              <not><equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_nomact"/></equal></not>
            </and>
          </test>
          <let><var n="main-verb-lemma"/><clip pos="1" side="tl" part="lem"/></let>
        </when>
      </choose>
     <choose>
        <when c="M[V] set subj_nbr when subj">
          <test>
          <and>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ→"/></equal>
              <or>
              <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="prn"/></equal>
              <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="n"/></equal>
              </or>
          </and>    
          </test>
          <let><var n="subj_nbr"/><clip pos="1" side="tl" part="a_nbr"/></let>
        </when>
        <when c="M[V] set empty subj_nbr when not subj">
          <test><and>
              <not><equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ→"/></equal></not>
              <or>
               <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="prn"/></equal>
              <equal><clip pos="1" side="sl" part="a_PoS"/><lit-tag v="n"/></equal>
               </or></and>
        </test>
          <let><var n="subj_nbr"/><lit v=""/></let>
        </when>
      </choose>
     <choose>
        <when c="M[V] set verb_tense_nbrpers">
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@+FAUXV"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@+FMAINV"/></equal>
            </or>
          </test>
          <let><var n="verb_tense_nbrpers"/><concat><clip pos="1" side="tl" part="a_tense"/><clip pos="1" side="tl" part="a_pers"/>
          <clip pos="1" side="tl" part="a_nbr"/></concat></let>
          <let><var n="tense"/><clip pos="1" side="tl" part="a_tense"/></let>
        </when>
        <when c="M[V] set infin">
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@-FAUXV"/></equal>
              <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@-FMAINV"/></equal>
            </or>
          </test>
          <let><var n="infin"/><clip pos="1" side="tl" part="a_infin"/></let>
        </when>
      </choose> 
     <choose>
        <when c="M[T] adds attr to prn.dem.pl.com">
          <test>
            <and>
              <equal><clip pos="1" side="sl" part="a_prn"/><lit-tag v="prn.dem"/></equal>
              <equal><clip pos="1" side="sl" part="a_nbr"/><lit-tag v="pl"/></equal>
              <equal><clip pos="1" side="sl" part="a_case"/><lit-tag v="com"/></equal>
            </and>
          </test>
          <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.dem.pl.com.attr"/></let>
        </when>
      </choose> 
     <choose>
      <when c="M[T] removes exstra tags  pcle">
      <test><equal><clip pos="1" side="tl" part="a_PoS"/><lit-tag v="pcle"/></equal></test>
          <let><clip pos="1" side="tl" part="tags"/><lit-tag v="pcle"/></let></when>
      </choose> 

      <!-- removing tags we don't need -->
      <let><clip pos="1" side="tl" part="a_spec_sme_tags"/><lit v=""/></let>
      <let><clip pos="1" side="tl" part="a_spec_sme_tags"/><lit v=""/></let> <!-- let be twice -->
      <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let>

      <call-macro n="convert-deriv-1Param">
        <with-param pos="1"/>
      </call-macro>
      <!-- If the word has a syntactic function label, set the variable with that
           label, if not, set the label to @X.
           Remove question particle. -->
      <let><var n="func"/><lit-tag v="@X"/></let>
      <choose>
        <when c="M[V] adds func">
          <test>
            <not><equal><clip pos="1" side="tl" part="a_func"/><lit v=""/></equal></not>
          </test>
          <let><var n="func"/><clip pos="1" side="tl" part="a_func"/></let>
        </when>
      </choose>
      <choose>
        <when c="M[V] adds ext">
          <test>
            <equal><clip pos="1" side="sl" part="a_exthab"/><lit-tag v="←ext→"/></equal>
          </test>
          <let><var n="ext"/><clip pos="1" side="sl" part="a_exthab"/></let>
        </when>
        <when  c="M[V] removes ext">
          <test>
            <not><equal><clip pos="1" side="sl" part="a_exthab"/><lit-tag v="←ext→"/></equal></not>
          </test>
          <let><var n="ext"/><lit v=""/></let>
        </when>
      </choose>
      <choose>
        <when c="M[T] cmp_sh to cmp_sgnom">
          <test>
            <equal><clip pos="1" side="tl" part="a_cmp"/><lit-tag v="cmp_sh"/></equal>
          </test>
      <let><clip pos="1" side="tl" part="a_cmp"/><lit-tag v="cmp_sgnom"/></let> 
        </when>
      </choose>
      <choose>
        <when c="M[T] removes a_attr when prfprc">
          <test>
            <equal><clip pos="1" side="tl" part="a_infin"/><lit-tag v="prfprc"/></equal>
          </test>
      <let><clip pos="1" side="tl" part="a_attr"/><lit v=""/></let> 
        </when>
      </choose>
      <choose>
        <when c="M[T] actio.loc to inf">
          <test><and>
            <equal><clip pos="1" side="tl" part="a_actio"/><lit-tag v="actio"/></equal>
            <equal><clip pos="1" side="tl" part="a_case"/><lit-tag v="loc"/></equal>
            </and>
          </test>
      <let><clip pos="1" side="tl" part="a_actio"/><lit-tag v="inf"/></let> 
      <let><clip pos="1" side="tl" part="a_case"/><lit v=""/></let> 
        </when>
