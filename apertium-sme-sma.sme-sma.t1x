         <call-macro n="general-macro-3Param"><with-param pos="1"/><with-param pos="3"/><with-param pos="2"/></call-macro>
        </otherwise>
     </choose>

     <choose>
     <when c="noun from sg to pl">
     <test><and>
     <not><equal><clip pos="2" side="sl" part="lem"/><lit v="okta"/></equal></not>
     <not><equal><clip pos="2" side="sl" part="lem"/><lit v="Okta"/></equal></not>
