    <rule comment="Subj FMAINV X X X" > <!-- V X X X becomes X X X V. -->
      <pattern>     
        <pattern-item n="IFMainV"/>
        <pattern-item n="Modifier_Argument_l"/>
        <pattern-item n="Modifier_Argument_l"/>
        <pattern-item n="Argument_l"/>
      </pattern>
      <action>
        <call-macro n="switch_cases"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <out>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="1"/>
           <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
          <b pos="2"/>
         <chunk>
            <clip pos="4" part="whole"/>
          </chunk>
          <b pos="3"/>
         <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="Subj FMAINV X X" > <!-- V X X becomes X X V. -->
      <pattern>     
        <pattern-item n="IFMainV"/>
        <pattern-item n="Modifier_Argument_l"/>
        <pattern-item n="Argument_l"/>
      </pattern>
      <action>
        <call-macro n="switch_cases"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <out>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="1"/>
           <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
          <b pos="2"/>
         <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="öövre gosk FMAINV " > <!-- V X X becomes X X V. -->
      <pattern>     
        <pattern-item n="öövre"/>
        <pattern-item n="FMainV"/>
      </pattern>
      <action>
        <out>
          <chunk>
           <get-case-from pos="1"><lit v="advl"/></get-case-from>
            <clip pos="1" part="tags"/>
           <lit v="{"/>
           <clip pos="1" part="content"/>
           <b pos="1"/>
              <lit v="^"/>
              <lit v="gosk"/>
              <lit-tag v="pcle"/>
              <lit v="$"/>
           <b pos="1"/>
           <clip pos="2" part="content"/>
           <lit v="}"/>
           <clip pos="1" part="lemq"/>
          </chunk>
        </out>
      </action>
    </rule>




